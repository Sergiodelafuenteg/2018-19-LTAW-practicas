{
  "name": "electron_prueba",
  "productName": "Aplicaciontoowapa",
  "version": "1.0.0",
  "description": "toowapo",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . Aplicaciontoowapa --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Aplicaciontoowapa --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",    
    "package-linux": "electron-packager . Aplicaciontoowapa --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "sergio",
  "license": "ISC",
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-packager": "^13.1.1"
  }
}
